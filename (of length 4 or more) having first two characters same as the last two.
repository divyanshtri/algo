void state1(string w, int i){ 
    cout<<"State 1"<<endl; 
    if(i==w.length()){ 
        cout<<"string is rejected"<<endl; 
        return; 
    }else{ 
        if(w[i]=='a'){ 
            state2(w, i+1); 
        }if(w[i]=='b'){ 
            state3(w, i+1); 
        } 
    } 
} 
 
